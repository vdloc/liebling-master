{
  "name": "liebling",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "get-database": "curl https://raw.githubusercontent.com/eddiesigner/ghost-db/master/ghost.db -o ghost.db",
    "docker-watch": "concurrently --names 'DOCKER,WEBPACK' --prefix-colors 'yellow,magenta' --kill-others \"docker-compose up\" \"npm run watch\"",
    "podman-watch": "podman-compose up -d && node node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "deploy": "node deploy/index.js",
    "build": "NODE_ENV=production node node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "zip": "run-script-os",
    "zip:win32": "cd .. && 7z a -r -x!.git -x!src -x!liebling.zip liebling.zip *",
    "zip:default": "cd .. && zip -r liebling.zip ./* -x '*.git*' -x '*src*' -x 'liebling.zip'",
    "order-locales": "node order-locales.js",
    "dev": "npm run development",
    "development": "mix",
    "watch": "concurrently --kill-others \"npm run tailwind-watch\" \"mix watch\"",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "tailwind-watch": "npx tailwindcss --postcss -i ./css/app.css -o ../assets/css/app.css --watch",
    "tailwind": "npx tailwindcss --postcss -i ./css/app.css -o ../assets/css/app.css --minify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run order-locales; git add ../locales"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "7zip": "0.0.6",
    "@tailwindcss/jit": "^0.1.18",
    "@tryghost/admin-api": "^1.4.0",
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "concurrently": "^6.5.1",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "ghost-cli": "^1.16.3",
    "husky": "^4.2.3",
    "laravel-mix": "^6.0.41",
    "path": "^0.12.7",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "resolve-url-loader": "3.1.2",
    "run-script-os": "^1.1.5",
    "sass": "^1.22.9",
    "sass-loader": "8.*",
    "tailwindcss": "^3.0.22",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@tryghost/content-api": "^1.6.0",
    "@tryghost/helpers": "^1.1.54",
    "aos": "2.3.4",
    "clipboard": "^2.0.8",
    "date-fns": "^2.28.0",
    "fitvids": "^2.0.0",
    "fuse.js": "^6.4.6",
    "headroom.js": "^0.12.0",
    "jquery": "^3.6.0",
    "lightgallery": "^1.8.3",
    "medium-zoom": "^1.0.6",
    "moment-timezone": "^0.5.34",
    "shave": "^2.5.10",
    "swiper": "^8.0.5",
    "tippy.js": "^5.2.1"
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}